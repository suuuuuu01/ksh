package sec08.exam01;

public class CalculatorExample {

	public static void main(String[] args) {
		
		// myCalc
		Calculator myCalc = new Calculator();
		
		// 계산기 전원  on
		myCalc.powerOn();
		
		// 5, 6
		int result1 = myCalc.plus(5, 6);
		System.out.println("result1 : " + result1);
		
		// 계산기 전원 Off
		myCalc.powerOff();
		
	}

}
