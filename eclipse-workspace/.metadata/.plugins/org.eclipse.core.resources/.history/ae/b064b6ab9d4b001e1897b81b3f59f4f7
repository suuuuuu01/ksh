package ch06.sec13.exam03.package1;

public class A {
	
	// 필드선언
	public int field1;
	
	int field2; // 접근자는 default 의미
	
	private int field3;
	
	// 생성자
	public A() {
		field1 = 1;
		field2 = 1;
		field3 = 1;
		
		method1();
		method2();
		method3();
	}
	
	public void method1() {}
	
	void method2() {} // default
	
	private void method3() {}
}
